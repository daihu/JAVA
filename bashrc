# .bashrc

# User specific aliases and functions

alias rm='rm -i'
alias cp='cp -i'
alias mv='mv -i'

# Source global definitions
if [ -f /etc/bashrc ]; then
        . /etc/bashrc
fi

alias dr='cd /usr/apache-tomcat-6.0.29/webapps/ROOT'
alias dl='cd /usr/apache-tomcat-6.0.29/logs'
~                                                